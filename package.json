{
  "name": "relexer",
  "version": "1.0.1",
  "description": "A regular expression based lexer for javascript and node.js streams",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "npm run build",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "prepare": "npm run build",
    "test": "mocha --require should dist/test"
  },
  "author": "Manish Vachharajani <manishv@unbounded.systems>",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.66",
    "@types/should": "11.2.0",
    "@types/stream-buffers": "3.0.3",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "stream-buffers": "3.0.2",
    "tslint": "5.20.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "make-error": "^1.3.0"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.map"
  ],
  "directories": {
    "test": "test"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unboundedsystems/relexer.git"
  },
  "keywords": [
    "parse",
    "lex",
    "node",
    "regexp",
    "regex",
    "node",
    "nodejs",
    "stream"
  ],
  "bugs": {
    "url": "https://github.com/unboundedsystems/relexer/issues"
  },
  "homepage": "https://github.com/unboundedsystems/relexer#readme"
}
